{
  "name": "miaoxing/wechat-tag",
  "description": "Miaoxing Wechat Tag Plugin",
  "keywords": [
    "miaoxing", "wechat", "tag"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "dev-master@dev"
  },
  "require-dev": {
    "miaoxing/coding-standards": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\WechatTag\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\WechatTag\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
